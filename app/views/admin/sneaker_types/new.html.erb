<div class="row">
  <h1>スニーカーモデル投稿フォーム</h1>
</div>
<%= form_with model: @sneaker_type, url: admin_sneaker_brand_sneaker_types_path(@sneaker_brand.id)  do |f| %>

  <%= render "layouts/error_messages", model: f.object %>

  <div class="row">
    <h4>ブランドネーム:<%= @sneaker_brand.name %></h4>
  </div>

  <div class="mb-2">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="mb-2">
    <%= f.label :overview %>
    <%= f.text_area :overview %>
  </div>

  <div class="mb-2">
    <%= f.submit '投稿' %>
  </div>
  
<% end %>
