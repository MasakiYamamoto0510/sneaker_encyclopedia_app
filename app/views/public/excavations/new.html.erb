<h2>新規発掘 投稿フォーム</h2>
<%= form_with model: @excavation do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>スニーカー名</h4>
  <%= f.text_field :sneaker_name %>
  <h4>ブランド名</h4>
  <%= f.text_field :brand_name %>
  <h4>概要</h4>
  <%= f.text_area :overview %>
  <h4>製造年</h4>
  <%= f.text_field :year_of_manufacture %>
  <h4>カラー</h4>
  <%= f.text_field :color %>
  <h4>サイズ</h4>
  <%= f.text_field:size %>
  <h4>備考</h4>
  <%= f.text_area:remarks %>
  <%= f.submit '投稿' %>
<% end %>